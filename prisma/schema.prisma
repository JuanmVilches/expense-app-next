generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int        @id @default(autoincrement())
  email     String     @unique
  name      String?
  password  String
  expenses  Expenses[]
  createdAt DateTime   @default(now())
}

model Expenses {
  id       Int      @id @default(autoincrement())
  expense  String
  amount   Int
  category String
  date     DateTime
  userId   Int
  user     User     @relation(fields: [userId], references: [id])
}
